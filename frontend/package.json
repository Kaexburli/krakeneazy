{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/static/svelte-material-ui/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/static/svelte-material-ui/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^3.0.1",
    "@smui-extra/badge": "^6.0.0-beta.14",
    "@smui/button": "^6.0.0-beta.14",
    "@smui/checkbox": "^6.0.0-beta.14",
    "@smui/data-table": "^6.0.0-beta.14",
    "@smui/dialog": "^6.0.0-beta.14",
    "@smui/drawer": "^6.0.0-beta.14",
    "@smui/form-field": "^6.0.0-beta.14",
    "@smui/layout-grid": "^6.0.0-beta.14",
    "@smui/linear-progress": "^6.0.0-beta.14",
    "@smui/list": "^6.0.0-beta.14",
    "@smui/radio": "^6.0.0-beta.14",
    "@smui/textfield": "^6.0.0-beta.14",
    "@zerodevx/svelte-toast": "^0.6.3",
    "dotenv": "^16.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "smui-theme": "^6.0.0-beta.14",
    "svelte": "^3.0.0",
    "svelte-loading-spinners": "^0.1.7"
  },
  "dependencies": {
    "@xstate/svelte": "^0.2.0",
    "immer": "^9.0.12",
    "jwt-decode": "^3.1.2",
    "lightweight-charts": "^3.7.0",
    "sirv-cli": "^1.0.0",
    "svelte-chartjs": "^1.1.2",
    "svelte-favicon-badge": "^1.0.0",
    "svelte-fetch": "^0.2.0",
    "svelte-i18n": "^3.3.13",
    "svelte-lightweight-charts": "^1.1.1",
    "svelte-material-ui": "^6.0.0-beta.14",
    "svelte-modals": "^1.0.4",
    "svelte-websocket-store": "^1.1.16",
    "xstate": "^4.27.0"
  }
}
